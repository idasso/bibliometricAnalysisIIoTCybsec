    {
      "cell_type": "code",
      "source": [
        "import re\n",
        "\n",
        "# Convert alike_keywords into a dictionary mapping each alike keyword to its main keyword\n",
        "keyword_dict = {}\n",
        "for _, row in keyword_mapping_create.iterrows():\n",
        "    main_keyword = row['main_keyword']\n",
        "    alike_keywords = row['alike_keywords'].split(', ')\n",
        "    for keyword in alike_keywords:\n",
        "        keyword_dict[keyword.strip()] = main_keyword\n",
        "\n",
        "# Function to replace alike_keywords with main_keyword\n",
        "def replace_keywords_strict(text, keyword_dict):\n",
        "    if isinstance(text, str):\n",
        "        # Sort keywords by length (longer keywords first to avoid partial matches)\n",
        "        for keyword in sorted(keyword_dict.keys(), key=len, reverse=True):\n",
        "            main_keyword = keyword_dict[keyword]\n",
        "            # Use word boundaries \\b for exact matches but handle special characters with re.escape\n",
        "            # Enforce spaces and other non-alphanumeric boundaries strictly\n",
        "            pattern = r'(?<!\\w)' + re.escape(keyword.strip()) + r'(?!\\w)'\n",
        "            #pattern = r'(?<!\\w|\\S)' + re.escape(keyword.strip()) + r'(?!\\w|\\S)'\n",
        "            text = re.sub(pattern, main_keyword, text)  # Strict and case-sensitive match\n",
        "    return text\n",
        "\n",
        "# Apply the function to columns 'Index Keywords' of keywordsRaw\n",
        "keywordsRaw['Index Keywords'] = keywordsRaw['Index Keywords'].apply(lambda x: replace_keywords_strict(x, keyword_dict))"
      ],
