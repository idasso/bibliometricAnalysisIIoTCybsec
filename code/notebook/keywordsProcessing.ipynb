    {
      "cell_type": "code",
      "source": [
        "from rapidfuzz import process, fuzz # Import related tools\n",
        "\n",
        "# Create an empty list to hold the groups\n",
        "grouped_keywords = []\n",
        "\n",
        "# Create a set to track keywords that have already been grouped\n",
        "grouped_set = set()\n",
        "\n",
        "# Iterate over each keyword and find similar ones\n",
        "for idx, row in filtered_keyword_counts.iterrows():\n",
        "    keyword = row['keyword']\n",
        "    if keyword not in grouped_set:\n",
        "        # Find similar keywords using rapidfuzz\n",
        "        similar_keywords = process.extract(keyword, filtered_keyword_counts['keyword'], scorer=fuzz.token_sort_ratio, limit=None)\n",
        "\n",
        "        # Filter similar keywords by a threshold (e.g., 90% similarity)\n",
        "        similar_group = [kw for kw, score, idx in similar_keywords if score >= 90 and kw not in grouped_set]\n",
        "\n",
        "        # Sum the counts for the grouped keywords\n",
        "        total_count = filtered_keyword_counts[filtered_keyword_counts['keyword'].isin(similar_group)]['count'].sum()\n",
        "\n",
        "        # Add the main keyword and its group to the list\n",
        "        grouped_keywords.append([keyword, ', '.join(similar_group), total_count])\n",
        "\n",
        "        # Add the grouped keywords to the set to avoid re-grouping\n",
        "        grouped_set.update(similar_group)\n",
        "\n",
        "# Step 4: Convert the result to a DataFrame\n",
        "grouped_df = pd.DataFrame(grouped_keywords, columns=['main_keyword', 'alike_keywords', 'total_count'])\n",
        "\n",
        "print(grouped_df)\n",
        "\n"
